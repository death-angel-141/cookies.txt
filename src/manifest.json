{
  "manifest_version": 3,
  "name": "cookie.txt",
  "version": "2",
  "description": "Exports your cookies super fast in a format compatible with wget",
  "short_name": "cookies",
  "offline_enabled": true,
  "icons": {
	"128": "icon.png"
   },
  "permissions": [ "activeTab", "cookies", "tabs", "webNavigation" ],
  "host_permissions": [ "http://*/*", "https://*/*" ],
  
  "action": {
    "default_title": "Export cookies.txt",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }  
}
